<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.importWizards">
      <category
            id="be.ac.vub.jar2uml.ui.sampleCategory"
            name="Jar2UML Import"/>
      <wizard
            category="be.ac.vub.jar2uml.ui.sampleCategory"
            class="be.ac.vub.jar2uml.ui.JarToUMLImportWizard"
            icon="icons/importjar_wiz.gif"
            id="be.ac.vub.jar2uml.ui.JarToUMLImportWizard"
            name="Import Jar Files to UML Models">
         <description>
            Import Jar files from the local file system into a UML model in the workspace.
         </description>
      </wizard>
      <wizard
            category="be.ac.vub.jar2uml.ui.sampleCategory"
            class="be.ac.vub.jar2uml.ui.JarToUMLImportDependenciesWizard"
            icon="icons/importjar_wiz.gif"
            id="be.ac.vub.jar2uml.ui.JarToUMLImportDependenciesWizard"
            name="Import Jar File Dependencies to UML Models">
         <description>
            Import dependencies of Jar files from the local file system into a UML model in the workspace.
         </description>
      </wizard>
      <wizard
            category="be.ac.vub.jar2uml.ui.sampleCategory"
            class="be.ac.vub.jar2uml.ui.JavaProjectToUMLImportWizard"
            icon="icons/importjar_wiz.gif"
            id="be.ac.vub.jar2uml.ui.JavaProjectToUMLImportWizard"
            name="Import Java projects to UML Models">
         <description>
            Import a Java project into a UML model in the workspace.
         </description>
      </wizard>
      <wizard
            category="be.ac.vub.jar2uml.ui.sampleCategory"
            class="be.ac.vub.jar2uml.ui.JavaProjectToUMLImportDependenciesWizard"
            icon="icons/importjar_wiz.gif"
            id="be.ac.vub.jar2uml.ui.JavaProjectToUMLImportDependenciesWizard"
            name="Import Java project dependencies to UML Models">
         <description>
            Import dependencies of a Java project into a UML model in the workspace.
         </description>
      </wizard>
   </extension>

</plugin>
