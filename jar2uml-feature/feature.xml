<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.eclipselabs.jar2uml"
      label="%featureName"
      version="2.0.0"
      provider-name="%providerName"
      plugin="org.eclipselabs.jar2uml">

   <description url="about.html">
      %description
   </description>

   <copyright url="about.html">
      %copyright
   </copyright>

   <license url="about.html">
      %license
   </license>

   <url>
      <update label="%updateSiteName" url="https://raw.githubusercontent.com/dwagelaar/jar2uml/master/update/"/>
   </url>

   <requires>
      <import plugin="org.junit"/>
      <import plugin="org.eclipse.ui.ide"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.jface"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.ui.console"/>
      <import plugin="org.eclipse.jdt.core"/>
      <import plugin="org.eclipse.uml2.uml" version="5.0.2" match="greaterOrEqual"/>
   </requires>

   <plugin
         id="org.eclipselabs.jar2uml"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.apache.bcel"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
